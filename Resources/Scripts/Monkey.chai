def OnBegin() {
   GetModelComponent(entity).mesh.Load("Resources/Models/torus.obj");
   GetModelComponent(entity).texture.Load("Resources/Textures/Bricks.jpg");
}

def OnUpdate(float delta) {
   GetTransformComponent(entity).position.x += 1 * delta;
   GetTransformComponent(entity).rotation.y += 100.0f  * delta;
}
